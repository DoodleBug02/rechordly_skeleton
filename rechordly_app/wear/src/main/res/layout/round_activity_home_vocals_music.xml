<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:background="@drawable/v_or_m"
    android:layout_height="match_parent"
    tools:context="me.chrisvle.rechordly.HomeVocalsMusicActivity" tools:deviceIds="wear_round">


    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:id="@+id/VocalsButton"
        android:background="@android:color/transparent"
        android:onClick="goToRecordActivity"
        android:layout_marginTop="46dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:id="@+id/MusicButton"
        android:background="@android:color/transparent"
        android:onClick="goToRecordActivity"
        android:layout_below="@+id/VocalsButton"
        android:layout_alignParentStart="true" />

</RelativeLayout>
